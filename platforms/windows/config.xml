<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.BusinessCardApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>BusinessCardApp</name>
    <description>
        Business Card App
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        dau-layer-8-solution
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <vs:toolsetVersion xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">6.3.1</vs:toolsetVersion>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="windows-target-version" value="10.0" />
</widget>
