    <mvc:View
        height="100%"
        controllerName="hs.fulda.customer.management.controller.CustomerDetail"
        xmlns="sap.m"
        xmlns:l="sap.ui.layout"
	    xmlns:form="sap.ui.layout.form"
        xmlns:blockcolor="sap.uxap.sample.ObjectPageFormLayout.block.blockscolor"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns:core="sap.ui.core">
        <ObjectHeader
		id="oh1"
		responsive="true"
		binding="{}"
		icon=""
		iconAlt="Test"
		intro="Test Beschreibung"
		title="Test Titel"
		showMarkers="false"
		markFlagged="true"
		markFavorite="true"
		backgroundDesign="Translucent">
		<attributes>
			<ObjectAttribute
				title="Manufacturer"
				text="Test Lieferant"/>
			<ObjectAttribute
				title="Dimension per unit"
				text="Width x Depth x Height"/>
		</attributes>
		<statuses>
			<ObjectStatus
				title="Approval"
				text="Pending"
				state="Warning"/>
		</statuses>
	</ObjectHeader>
    </mvc:View>
